{
  "name": "angular2-express",
  "version": "1.0.0",
  "description": "Angular2 Gulp Typescript and Express",
  "main": "dist/server.js",
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "compile": "./node_modules/.bin/gulp compile",
    "build": "./node_modules/.bin/gulp build",
    "start": "concurrent --kill-others \"./node_modules/.bin/gulp watch\" \"./node_modules/.bin/gulp start\"",
    "postinstall": "./node_modules/.bin/gulp installTypings"
  },
  "keywords": [
    "Angular2",
    "Express",
    "Quick Start",
    "Gulp",
    "Typescript"
  ],
  "author": "Moiz Kachwala <moizk@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "@angular/upgrade": "4.0.0",
    "core-js": "^2.4.0",
    "express": "^4.13.4",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.1",
    "systemjs": "0.19.27",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^3.1.6",
    "gulp-typings": "^2.0.4",
    "run-sequence": "^1.2.1",
    "ts-node": "^3.0.4",
    "tslint": "^3.5.0",
    "typescript": "^2.3.2"
  }
